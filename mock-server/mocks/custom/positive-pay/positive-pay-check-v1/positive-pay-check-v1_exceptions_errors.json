{
  "name": "/positive-pay-check/client-api/v1/exceptions/errors",
  "request": {
    "url": "/positive-pay-check/client-api/v1/exceptions/errors(\\?.*)?$",
    "method": "POST"
  },
  "responses": {
    "badRequest": {
      "status": 400,
      "data": {
        "message": "Bad Request",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Value Exceeded. Must be between {min} and {max}.",
            "key": "common.api.shoesize",
            "context": {
              "max": "50",
              "min": "1"
            }
          }
        ]
      }
    },
    "unathorized": {
      "status": 401,
      "data": {
        "errors": [
          {
            "message": "Unathorized",
            "key": "common.api.unathorized"
          }
        ]
      }
    },
    "forbidden": {
      "status": 403,
      "data": {
        "message": "Access to requested resource denied.",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Resource access denied due to an insufficient user quota of {quota}.",
            "key": "common.api.quota",
            "context": {
              "quota": "someQuota"
            }
          }
        ]
      }
    },
    "systemError": {
      "status": 500,
      "data": {
        "message": "Description of error"
      }
    },
    "success": {
      "default": true,
      "status": 200,
      "data": {
        "errors": ["Amount mismatch", "Already paid", "Duplicate check"]
      }
    }
  }
}
