{
  "name": "/client-api/v2/account/statements/download/{uid}_GET",
  "request": {
    "url": "/client-api/v2/account/statements/download/[^/]+(\\?.*)?$",
    "method": "GET"
  },
  "responses": {
    "notFound": {
      "status": 404,
      "data": {
        "message": "Resource not found.",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Unable to find the resource requested resource: {resource}.",
            "key": "common.api.resource",
            "context": {
              "resource": "aResource"
            }
          }
        ]
      }
    },
    "badRequest": {
      "status": 400,
      "data": {
        "message": "Bad Request",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Value Exceeded. Must be between {min} and {max}.",
            "key": "common.api.shoesize",
            "context": {
              "max": "50",
              "min": "1"
            }
          }
        ]
      }
    },
    "systemError": {
      "status": 500,
      "data": {
        "message": "Description of error"
      }
    },
    "forbidden": {
      "status": 403,
      "data": {
        "message": "Access to requested resource denied.",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Resource access denied due to an insufficient user quota of {quota}.",
            "key": "common.api.quota",
            "context": {
              "quota": "someQuota"
            }
          }
        ]
      }
    }
  }
}
