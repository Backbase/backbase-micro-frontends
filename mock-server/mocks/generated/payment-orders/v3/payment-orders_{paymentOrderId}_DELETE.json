{
  "name": "/client-api/v3/payment-orders/{paymentOrderId}_DELETE",
  "request": {
    "url": "/client-api/v3/payment-orders/[^/]+(\\?.*)?$",
    "method": "DELETE"
  },
  "responses": {
    "success": {
      "status": 204,
      "data": "",
      "default": true
    },
    "badRequest": {
      "status": 400,
      "data": {
        "message": "Bad Request",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Value Exceeded. Must be between {min} and {max}.",
            "key": "common.api.shoesize",
            "context": {
              "max": "50",
              "min": "1"
            }
          }
        ]
      }
    },
    "forbidden": {
      "status": 403,
      "data": {
        "message": "Access to requested resource denied.",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Resource access denied due to an insufficient user quota of {quota}.",
            "key": "common.api.quota",
            "context": {
              "quota": "someQuota"
            }
          }
        ]
      }
    }
  }
}
