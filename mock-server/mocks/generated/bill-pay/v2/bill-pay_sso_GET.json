{
  "name": "/client-api/v2/bill-pay/sso_GET",
  "request": {
    "url": "/client-api/v2/bill-pay/sso(\\?.*)?$",
    "method": "GET"
  },
  "responses": {
    "success": {
      "status": 200,
      "data": {
        "ssoUrl": "https://test-billcenter-ui-api.regrpayverisbp.com/render/billcenter?clientCode=backbase_clientcode&artifactId=session-artifact-id"
      },
      "default": true
    },
    "badRequest": {
      "status": 400,
      "data": {
        "message": "Bad Request",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Value Exceeded. Must be between {min} and {max}.",
            "key": "common.api.shoesize",
            "context": {
              "max": "50",
              "min": "1"
            }
          }
        ]
      }
    },
    "notAuthorized": {
      "status": 401,
      "data": {
        "message": "Access to requested resource denied.",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Resource access denied due to invalid credentials.",
            "key": "common.api.token",
            "context": {
              "accessToken": "expired"
            }
          }
        ]
      }
    },
    "forbidden": {
      "status": 403,
      "data": {
        "message": "Access to requested resource denied.",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Resource access denied due to an insufficient user quota of {quota}.",
            "key": "common.api.quota",
            "context": {
              "quota": "someQuota"
            }
          }
        ]
      }
    }
  }
}
