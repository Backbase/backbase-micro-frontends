{
  "name": "/client-api/v2/users/identities/{internalId}/actions_PUT",
  "request": {
    "url": "/client-api/v2/users/identities/[^/]+/actions(\\?.*)?$",
    "method": "PUT"
  },
  "responses": {
    "success": {
      "status": 204,
      "data": "",
      "default": true
    },
    "badRequest": {
      "status": 400,
      "data": {
        "message": "Bad Request",
        "errors": [
          {
            "message": "Value Exceeded. Must be between {min} and {max}.",
            "key": "common.api.shoesize",
            "context": {
              "max": "50",
              "min": "1"
            }
          }
        ]
      }
    },
    "forbidden": {
      "status": 403,
      "data": {
        "message": "Access to requested resource denied.",
        "errors": [
          {
            "message": "Resource access denied due to an insufficient user quota of {quota}.",
            "key": "common.api.quota",
            "context": {
              "quota": "someQuota"
            }
          }
        ]
      }
    },
    "notFound": {
      "status": 404,
      "data": {
        "message": "Resource not found.",
        "errors": [
          {
            "message": "Unable to find the resource requested resource: {resource}.",
            "key": "common.api.resource",
            "context": {
              "resource": "aResource"
            }
          }
        ]
      }
    }
  }
}
