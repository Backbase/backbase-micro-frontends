{
  "name": "/client-api/v5/employee/mailouts/preview_POST",
  "request": {
    "url": "/client-api/v5/employee/mailouts/preview(\\?.*)?$",
    "method": "POST"
  },
  "responses": {
    "success": {
      "status": 201,
      "data": {
        "body": ""
      },
      "default": true
    },
    "badRequest": {
      "status": 400,
      "data": {
        "message": "Bad Request",
        "errors": [
          {
            "message": "Value Exceeded. Must be between {min} and {max}.",
            "key": "common.api.shoesize",
            "context": {
              "max": "50",
              "min": "1"
            }
          }
        ]
      }
    }
  }
}
